if [[ "$TARGET" =~ i[3-6]86 ]]; then
    PROC="x86"
else
    PROC="$TARGET"
fi
CONFIGURE=""
MAKEBUILD="make -C base $MAKEOPTS -j1 CARMEN_HOME=$BUILDROOT/carmen PROCESSOR=$PROC ROBOT_TYPES=robox"
MAKEINSTALL=("mkdir -p $EPREFIX/etc/init.d"
             "cp -ua $CONFDIR/robox-carmen.$TARGET $EPREFIX/etc/carmen.conf"
             "cp -a carmen.sh $EPREFIX/etc/init.d"
             "mkdir -p $PREFIX/bin"
             "cp -a base/robox $PREFIX/bin")
COMMENT="takes less than a minute"
