ALIAS="gcc"
PATH="$EPREFIX/bin:$PATH"
CONFIGURE="./configure --prefix=$PREFIX --exec-prefix=$EPREFIX --target=$TARGET-linux --with-sysroot=$EPREFIX --enable-languages=c --disable-shared --disable-threads --disable-multilib --disable-nls"
