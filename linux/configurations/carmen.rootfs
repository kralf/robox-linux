BUILDDIR="./src"
if [[ "$TARGET" =~ "i[3-6]86" ]]; then
    PROC="x86"
else
    PROC="$TARGET"
fi
CONFIGURE=("rm -rf laser"
           "./configure --prefix=$USRROOT --nographics --nojoystick")
MAKEBUILD="make $MAKEOPTS CARMEN_HOME=$PKGBUILDROOT PROCESSOR=$PROC"
MAKEINSTALL=("mkdir -p $USRROOT/bin"
             "cp -a laser/laser $USRROOT/bin")
COMMENT="takes just 2 minutes"

